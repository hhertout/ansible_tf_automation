        {
          "datasource": {
            "type": "loki",
            "uid": "${ds_logs}"
          },
          "direction": "backward",
          "editorMode": "code",
          "expr": "sum by (wait_event_type) (label_replace(label_replace(label_replace(label_replace(label_replace(label_replace(sum_over_time({job=\"$job\", op=\"wait_event\", server_id!=\"\"} | logfmt | wait_time>=0ms | (wait_event_type=~\"(IO)|(Lock|LWLock|Activity|Extension|InjectionPoint|IPC|Timeout|BufferPin)|(Client)\" or wait_event_name=~\"(wait/io/(file|table).+)|(wait/((io/lock|synch)|lock).+)|(wait/io/socket.+)\") | unwrap duration_seconds(wait_time)[$__auto]), \"wait_event_type\", \"IO Wait\", \"wait_event_type\", \"IO\"), \"wait_event_type\", \"Lock Wait\", \"wait_event_type\", \"Lock|LWLock|Activity|Extension|InjectionPoint|IPC|Timeout|BufferPin\"), \"wait_event_type\", \"Network Wait\", \"wait_event_type\", \"Client\"), \"wait_event_type\", \"IO Wait\", \"wait_event_name\", \"wait/io/(file|table).+\"), \"wait_event_type\", \"Lock Wait\", \"wait_event_name\", \"wait/((io/lock|synch)|lock).+\"), \"wait_event_type\", \"Network Wait\", \"wait_event_name\", \"wait/io/socket.+\")) > 0",
          "legendFormat": "{{wait_event_type}}",
          "queryType": "range",
          "refId": "A"
        }
      ],
